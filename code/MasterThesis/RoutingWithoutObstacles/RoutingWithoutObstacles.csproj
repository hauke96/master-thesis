<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>disable</Nullable>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Mars.Base" Version="4.3.1-beta-3" />
      <PackageReference Include="Mars.Common" Version="4.3.1-beta-3" />
      <PackageReference Include="Mars.Components" Version="4.3.1-beta-3" />
      <PackageReference Include="Mars.Core" Version="4.3.1-beta-3" />
      <PackageReference Include="Mars.Interfaces" Version="4.3.1-beta-3" />
      <PackageReference Include="Mars.IO" Version="4.3.1-beta-3" />
      <PackageReference Include="Mars.Numerics" Version="4.3.1-beta-3" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="config.json" />
      <Content Include="config.json">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Content>
    </ItemGroup>

</Project>
